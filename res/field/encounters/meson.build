pl_enc_tbl_narc = custom_target('pl_enc_data.narc',
    output: [
        'pl_enc_data.narc',
        'pl_enc_data.naix.h',
    ],
    input: [
        enc_bin_gen.process(pl_enc_data_srcs, env: json2bin_env),
        pl_enc_data_order
    ],
    depends: [ py_consts_generators ],
    command: [
        narc_exe, 'create',
        '--naix',
        '--order', pl_enc_data_order,
        '--output', '@OUTPUT0@',
        '@PRIVATE_DIR@',
    ]
)

