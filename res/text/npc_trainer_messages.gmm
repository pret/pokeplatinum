	<row id="NPCTrainerMessages_Text_Unknown150_PostBattle" index="751">
		<attribute name="window_context_name">garbage</attribute>
		<language name="English"></language>
		<language name="日本語">XXXXXXXXXXXXXXXXX</language>
	</row>
	<row id="NPCTrainerMessages_Text_Unknown151_PreBattle" index="752">
		<attribute name="window_context_name">garbage</attribute>
		<language name="English"></language>
		<language name="日本語">XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX</language>
	</row>
	<row id="NPCTrainerMessages_Text_Unknown151_Defeat" index="753">
		<attribute name="window_context_name">garbage</attribute>
		<language name="English"></language>
		<language name="日本語">XXXXXXXXX</language>
	</row>
	<row id="NPCTrainerMessages_Text_Unknown151_PostBattle" index="754">
		<attribute name="window_context_name">garbage</attribute>
		<language name="English"></language>
		<language name="日本語">XXXXXXXXXXXXXXXXXXXXXXXXXX</language>
	</row>
	<row id="NPCTrainerMessages_Text_Unknown152_PreBattle" index="755">
		<attribute name="window_context_name">garbage</attribute>
		<language name="English"></language>
		<language name="日本語">XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX</language>
	</row>
	<row id="NPCTrainerMessages_Text_Unknown152_Defeat" index="756">
		<attribute name="window_context_name">garbage</attribute>
		<language name="English"></language>
		<language name="日本語">XXXXXXXXXXXX</language>
	</row>
	<row id="NPCTrainerMessages_Text_Unknown152_PostBattle" index="757">
		<attribute name="window_context_name">garbage</attribute>
		<language name="English"></language>
		<language name="日本語">XXXXXXXXXXXXXXXXXXXXXXX</language>
	</row>
	<row id="NPCTrainerMessages_Text_FishermanLuc_PreBattle" index="758">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Yeah, yeah...\nNothing’s biting!\r</language>
	</row>
	<row id="NPCTrainerMessages_Text_FishermanLuc_Defeat" index="759">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Can’t catch a fish!\nCan’t catch a break!\n</language>
	</row>
	<row id="NPCTrainerMessages_Text_FishermanLuc_PostBattle" index="760">
		<attribute name="window_context_name">used</attribute>
		<language name="English">I was counting on strength in numbers,\nbut...\rPacking it in and going home sounds good\nat a time like this...\n</language>
	</row>
	<row id="NPCTrainerMessages_Text_FishermanLuc_Rematch" index="761">
		<attribute name="window_context_name">used</attribute>
