	<row id="NPCTrainerMessages_Text_RivalCanalaveCityPiplup_LastBattler" index="1949">
		<attribute name="window_context_name">used</attribute>
		<language name="English">We’ve gotten out of tighter spots\nthan this plenty of times before!\n</language>
	</row>
	<row id="NPCTrainerMessages_Text_RivalCanalaveCityPiplup_LastBattlerHalfHP" index="1950">
