	<row id="NPCTrainerMessages_Text_PokeKidDanielle_PreBattle" index="232">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Hi, hi!\nI’m a Pokémon!\r</language>
	</row>
	<row id="NPCTrainerMessages_Text_PokeKidDanielle_Defeat" index="233">
		<attribute name="window_context_name">used</attribute>
		<language name="English">So sad...\nSo bitter...\n</language>
	</row>
	<row id="NPCTrainerMessages_Text_PokeKidDanielle_PostBattle" index="234">
		<attribute name="window_context_name">used</attribute>
		<language name="English">I don’t want to be a Trainer.\nI want to be a Pokémon!\n</language>
	</row>
	<row id="NPCTrainerMessages_Text_PokeKidDanielle_Rematch" index="235">
		<attribute name="window_context_name">used</attribute>
		<language name="English">Hi, hi!\nI’m a Pokémon!\r...You knew that, didn’t you?\r</language>
	</row>
